



Kadane's Alogrithm
class Solution {
    public int maxSubArray(int[] nums) {
        int curr=0;
        int mx_sum=Integer.MIN_VALUE;

        for(int i=0;i<nums.length;i++)
        {
            curr=curr+nums[i];
            if(curr>mx_sum)
            {
                mx_sum=curr;
            }

            if(curr<0)
            {
                curr=0;
            }
        }

        return mx_sum;
        
    }
}
